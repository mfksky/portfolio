---
import { Image, Picture } from 'astro:assets';
import srcImage from '../images/main/me.jpg';

import Static from '../layouts/Static.astro'

import '../styles/main/style.scss';
---

<Static>
	<main class="page">
        <h1>Test</h1>
        <Picture
            src={srcImage}
            alt='Test'
            width={512}
            height={512}
            formats={['avif', 'webp']}
            fallbackFormat='jpg'
        />
	</main>
</Static>